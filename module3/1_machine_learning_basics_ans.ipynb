    "def g(x, w):\n",
    "    \n",
    "    X = np.zeros((len(x), len(w)))\n",
    "    for i in range(len(w)): \n",
    "        X[:,i] = x**i\n",
    "        \n",
    "    return np.sum(np.dot(X, w), axis=1)\n",
    "\n",
    "def loss(x, y, w):\n",
    "    \n",
    "    return 0.5/len(x)*np.sum((g(x, w)-y)**2)\n",
    "\n",
    "x = np.array([1.5, 5.3, 3.9, 9.4, 5.5])\n",
    "y = np.array([4.4, 2.8, 6.1, 7.5, 2.3])\n",
    "w = np.array([[0.7],\n",
    "              [0.8],\n",
