    "def g(x, w):\n",
    "    p = len(w) - 1\n",
    "    X = np.empty((len(x), p + 1))\n",
    "    for i in range(p + 1): \n",
    "        X[:, i] = x**i\n",
    "    y = np.dot(X, w).squeeze()\n",
    "    return y\n",
    "\n",
    "def loss(x, y, w):\n",
    "    l = 0.5 * np.mean((y - g(x, w))**2)\n",
    "    return l"
